/dts-v1/;

/ {
  #address-cells = <1>;
  #size-cells = <1>;

    chosen {
      opensbi-config {
        compatible = "opensbi,config";
        stdout-path = "/serial@10000000";
      };
    };

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x10000000>;
	};
    
    cpus {
      #address-cells = <1>;
      #size-cells = <0>;
		  timebase-frequency = <0x989680>;
    
      cpu0: cpu@0 {
          device_type = "cpu";
          status = "okay";
          riscv,isa-base = "rv32i";
          riscv,isa = "rv32imazicsr_zifencei_zicntr_zmmul_zaamo_zalrsc";
          mmu-type = "riscv,sv32";
          reg = <0>;
          compatible = "riscv";
      };
    };
    
    uart0: serial@10000000 {
      compatible = "ns16550a";
      reg = <0x10000000 0x100>;
    };
};
